generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model MovieRequests {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  added     Boolean
  fullName  String
  email     String
  movieName String
  createdAt DateTime @default(now())
}
